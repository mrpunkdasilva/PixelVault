{
  "name": "pixelvault",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "eslint-plugin-react-perf": "^3.3.3",
    "firebase": "^11.9.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:optimized": "npm run build && gulp optimize",
    "build:production": "npm run build && gulp",
    "optimize": "gulp optimize",
    "optimize:images": "gulp optimize:images",
    "analyze:bundle": "gulp analyze",
    "check:budget": "gulp budget",
    "preview": "vite preview",
    "bundle-analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss,json}\"",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "ci:check": "npm run type-check && npm run lint && npm run format:check"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^9.29.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gulp": "^5.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^9.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.83.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5"
  }
}
